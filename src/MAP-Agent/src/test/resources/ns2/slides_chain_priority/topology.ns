# Generated by khoihd

set ns [new Simulator]
source tb_compat.tcl

# Clients
set clientPoolD [$ns node]
tb-set-node-os $clientPoolD XEN46-64-STD

# NCPs
set nodeA [$ns node]
tb-set-node-os $nodeA XEN46-64-STD
tb-set-hardware $nodeA A

set nodeB [$ns node]
tb-set-node-os $nodeB XEN46-64-STD
tb-set-hardware $nodeB B

set nodeC [$ns node]
tb-set-node-os $nodeC XEN46-64-STD
tb-set-hardware $nodeC C

set nodeD [$ns node]
tb-set-node-os $nodeD XEN46-64-STD
tb-set-hardware $nodeD D


# Links
set linkAB [$ns duplex-link $nodeA $nodeB 100000.0kb 0.0ms DropTail]
set linkBC [$ns duplex-link $nodeB $nodeC 100000.0kb 0.0ms DropTail]
set linkCD [$ns duplex-link $nodeC $nodeD 100000.0kb 0.0ms DropTail]

set linkClientD [$ns duplex-link $clientPoolD $nodeD 100000.0kb 0.0ms DropTail]

$ns rtproto Static
$ns run
