# Generated by NetlabClient

set ns [new Simulator]
source tb_compat.tcl

# Clients
set clientPool1 [$ns node]
tb-set-node-os $clientPool1 XEN46-64-STD

# Servers
set serverX [$ns node]
tb-set-node-os $serverX XEN46-64-STD
tb-set-hardware $serverX X

# NCPs
set nodeA0 [$ns node]
tb-set-node-os $nodeA0 XEN46-64-STD
tb-set-hardware $nodeA0 A

set nodeB0 [$ns node]
tb-set-node-os $nodeB0 XEN46-64-STD
tb-set-hardware $nodeB0 B

set nodeC0 [$ns node]
tb-set-node-os $nodeC0 XEN46-64-STD
tb-set-hardware $nodeC0 C

set nodeD0 [$ns node]
tb-set-node-os $nodeD0 XEN46-64-STD
tb-set-hardware $nodeD0 D

# Links
set linkAD [$ns duplex-link $nodeA0 $nodeD0 1000000.0kb 0.0ms DropTail]
set linkDC [$ns duplex-link $nodeD0 $nodeC0 1000000.0kb 0.0ms DropTail]
set linkCB [$ns duplex-link $nodeC0 $nodeB0 1000000.0kb 0.0ms DropTail]
set linkBX [$ns duplex-link $nodeB0 $serverX 1000000.0kb 0.0ms DropTail]
set linkClienPool1A [$ns duplex-link $clientPool1 $nodeA0 1000000.0kb 0.0ms DropTail]

$ns rtproto Static
$ns run
