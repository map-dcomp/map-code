# Generated by NetlabClient

set ns [new Simulator]
source tb_compat.tcl

# Clients
set clientPool1 [$ns node]
tb-set-node-os $clientPool1 XEN46-64-STD

# Servers
set serverA [$ns node]
tb-set-node-os $serverA XEN46-64-STD
tb-set-hardware $serverA A

# NCPs
set nodeB0 [$ns node]
tb-set-node-os $nodeB0 XEN46-64-STD
tb-set-hardware $nodeB0 B

set nodeC0 [$ns node]
tb-set-node-os $nodeC0 XEN46-64-STD
tb-set-hardware $nodeC0 C

# Links
set linkAB [$ns duplex-link $serverA $nodeB0 1000000.0kb 0.0ms DropTail]
set linkBC [$ns duplex-link $nodeB0 $nodeC0 1000000.0kb 0.0ms DropTail]
set linkClienPool1C [$ns duplex-link $clientPool1 $nodeC0 1000000.0kb 0.0ms DropTail]

$ns rtproto Static
$ns run
