# Generated by khoihd

set ns [new Simulator]
source tb_compat.tcl

# Clients
set clientPoolF [$ns node]
tb-set-node-os $clientPoolF XEN46-64-STD

# NCPs
set nodeA [$ns node]
tb-set-node-os $nodeA XEN46-64-STD
tb-set-hardware $nodeA A

set nodeB [$ns node]
tb-set-node-os $nodeB XEN46-64-STD
tb-set-hardware $nodeB B

set nodeD [$ns node]
tb-set-node-os $nodeD XEN46-64-STD
tb-set-hardware $nodeD D

set nodeE [$ns node]
tb-set-node-os $nodeE XEN46-64-STD
tb-set-hardware $nodeE E

set serverF [$ns node]
tb-set-node-os $serverF XEN46-64-STD
tb-set-hardware $serverF F

# Links
set linkAC [$ns duplex-link $nodeA $nodeE 100000.0kb 0.0ms DropTail]
set linkBC [$ns duplex-link $nodeB $nodeE 100000.0kb 0.0ms DropTail]
set linkDE [$ns duplex-link $nodeD $nodeE 100000.0kb 0.0ms DropTail]
set linkEF [$ns duplex-link $nodeE $serverF 100000.0kb 0.0ms DropTail]
set linkClientF [$ns duplex-link $clientPoolF $serverF 100000.0kb 0.0ms DropTail]

$ns rtproto Static
$ns run
