# Generated by NetlabClient

set ns [new Simulator]
source tb_compat.tcl

# Clients
set clientPoolA [$ns node]
tb-set-node-os $clientPoolA XEN46-64-STD

# Servers
set serverX [$ns node]
tb-set-node-os $serverX XEN46-64-STD
tb-set-hardware $serverX X

# NCPs
set nodeA0 [$ns node]
tb-set-node-os $nodeA0 XEN46-64-STD
tb-set-hardware $nodeA0 A

set nodeB0 [$ns node]
tb-set-node-os $nodeB0 XEN46-64-STD
tb-set-hardware $nodeB0 B

set nodeC0 [$ns node]
tb-set-node-os $nodeC0 XEN46-64-STD
tb-set-hardware $nodeC0 C


# Links
set linkXC [$ns duplex-link $serverX $nodeC0 1000000.0kb 0.0ms DropTail]
set linkCB [$ns duplex-link $nodeC0 $nodeB0 1000000.0kb 0.0ms DropTail]
set linkBA [$ns duplex-link $nodeB0 $nodeA0 1000000.0kb 0.0ms DropTail]
set linkClientPoolAA [$ns duplex-link $clientPoolA $nodeA0 1000000.0kb 0.0ms DropTail]

$ns rtproto Static
$ns run
